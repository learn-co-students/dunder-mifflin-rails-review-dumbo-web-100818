<h1>Edit Employee</h1>
<%=form_for @employee do |f|%>
<%=f.label :alias%><br>
<%=f.text_field :alias%><br>
<%=f.label :title%><br>
<%=f.text_field :title%><br>
<%=f.label :office%><br>
<%=f.text_field :office%><br>
<%=f.label :img_url%><br>
<%=f.text_field :img_url%><br>
<%=f.submit "Edit Employee"%>
<%end%>
